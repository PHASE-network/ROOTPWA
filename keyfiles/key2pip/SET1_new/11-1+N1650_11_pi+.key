productionVertex :	
{		
  type = "diffractiveDissVertex";
 beam : {
    name = "p+";
  };
 target : {
    name = "p+";
  };
};


wave :
{
  
 XQuantumNumbers :
  {
    isospin =  1;
    J       =  1;
    P       = -1;
    M       =  1;
    refl    = +1;
  };
  
 XDecay :
  {
    isobars = ({
	name = "N(1650)";
	fsParticles = ({
	    name = "p+";
	  },
	  {
	    name = "pi-";
	  });
	L = 0;
	S = 1;
      });
    L = 2;
    S = 1;
    fsParticles = ({
	name = "pi+";
      });
  };
  
};


amplitude :
{
  formalism = "canonical";
};
