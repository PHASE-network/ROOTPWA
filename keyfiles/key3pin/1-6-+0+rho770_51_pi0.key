//-------------------------------------------------------------------------
//
// Description:
//      pi- pi0 pi0 final state
//      IG JPC MR Isobar1    [L S] Isobar2
//      1- 6-+ 0+ rho(770)   [5 1] pi0
//
//      !NOTE! all angular momenta and projection quantum numbers are
//             in units of hbar/2
//
//
// Author List:
//      Sebastian Uhl        TUM            (original author)
//
//
//-------------------------------------------------------------------------


productionVertex :
{
	type = "diffractiveDissVertex";
  beam : {
		name = "pi-";
	};
  target : {
		name = "p+";
	};
};


decayVertex :
{

  XQuantumNumbers :
	{
		isospin =  2;
		G       = -1;
		J       = 12;
		P       = -1;
		M       =  0;
		refl    = +1;
	};

  XDecay :
	{
		isobars = ({
				name = "rho(770)-";
				fsParticles = ({
						name = "pi-";
					},
					{
						name = "pi0";
					});
				L = 2;
				S = 0;
			});
		L = 10;
		S = 2;
		fsParticles = ({
				name = "pi0";
			});
	};

};
