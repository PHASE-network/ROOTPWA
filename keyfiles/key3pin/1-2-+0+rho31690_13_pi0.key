productionVertex :
{
    type = "diffractiveDissVertex";
    beam :
    {
        name = "pi-";
    };
    target :
    {
        name = "p+";
    };
};


decayVertex :
{
    XQuantumNumbers :
    {
        isospin =  2;
        G       = -1;
        J       =  4;
        P       = -1;
        M       =  0;
        refl    = +1;
    };
  
    XDecay :
    {
        isobars = ({
            name = "rho3(1690)";
            fsParticles = ({
                name = "pi-";
            }, {
                name = "pi0";
            });
            L = 6;
            S = 0;
        });
        L = 2;
        S = 6;
        fsParticles = ({
            name = "pi0";
        });
    };
};
