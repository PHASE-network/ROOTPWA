productionVertex :
{
	type = "diffractiveDissociation";
	particles = ({
			name = "pi-";
		});
};


wave :
{

  XQuantumNumbers :
	{
		isospin =  2;
		G       = -1;
		J       =  2;
		P       = +1;
		C       = +1;
		M       =  2;
		refl    = -1;
	};
  
  XDecay :
	{
		isobars = ({
				name = "rho(770)";
				fsParticles = ({
						name = "pi-";
					},
					{
						name = "pi+";
					});
				L = 2;
				S = 0;
			});
		L = 0;
		S = 2;
		fsParticles = ({
				name = "pi-";
			});
	};

};
