productionVertex :
{
	type = "diffractiveDissociation";
  beam : {
		name = "pi-";
	};
  target : {
		name = "p+";
	};
};


wave :
{

  XQuantumNumbers :
	{
		isospin =  2;
		G       = -1;
		J       =  6;
		P       = +1;
		C       = +1;
		M       =  2;
		refl    = +1;
	};
  
  XDecay :
	{
		isobars = ({
				name = "rho3(1690)";
				fsParticles = ({
						name = "pi-";
					},
					{
						name = "pi+";
					});
				L = 6;
				S = 0;
			});
		L = 0;
		S = 6;
		fsParticles = ({
				name = "pi-";
			});
	};

};
