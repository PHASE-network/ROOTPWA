productionVertex : 
{
	type = "diffractiveDissVertex";
	beam : 
	{
		name = "pi-";
	};
	target : 
	{
		name = "p+";
	};
};


decayVertex : 
{
	XDecay : 
	{
		isobars = ( 
			{
				name = "isobarA";
				fsParticles = ( 
					{
						name  = "eta";
					}, 
					{
						name  = "eta";
					} );
			} );
		fsParticles = ( 
			{
				name  = "pi-";
			} );
	};
};


waveSetParameters :
{
	// X quantum number ranges
	isospinRange     = [0, 2];
	JRange           = [0, 4];
	MRange           = [0, 0];
	reflectivity     = +1;
	useReflectivity  = true;
	allowSpinExotics = true;

	// X and isobar decay quantum number ranges
	LRange = [0, 4];
	SRange = [0, 4];

@include "IsobarWhiteList.key"
};
