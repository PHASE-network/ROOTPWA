productionVertex:
{
  type          = "diffractiveDissociation";
  beamParticles = ({
                     name = "pi-";
                  });
};


wave:
{

  options:
  {
    boseSymmetrize       = false;
    useReflectivityBasis = true;
  };

  XQuantumNumbers:
  {
    isospin =  2;
    G       = -1;
    J       =  4;
    P       = +1;
    C       = +1;
    M       =  2;
    refl    = +1;
  };
  
  XDecay:
  {
    fsParticles = ({
                     name     = "pi-";
		     index    = 0;
		     spinProj = 0;
                  });
    isobars     = ({
	             name        = "f1(1285)";
		     fsParticles = ({
			              name  = "pi-";
				      index = 1;
		                   });
		     isobars     = ({
		                      name        = "a1(1269)";
				      fsParticles = ({
				                       name  = "pi+";
						       index = 0;
					            });
				      isobars     = ({
				                       name        = "sigma";
						       massDep     = "amp_ves";
						       fsParticles = ({
								        name  = "pi-";
									index = 2;
							              },
							              {
									name  = "pi+";
									index = 1;
								      });
						       L = 0;
						       S = 0;
					            });
				      L = 2;
				      S = 0;
		                   });
		     L = 2;
		     S = 2;
                  });
    L = 2;
    S = 2;
  };

};
