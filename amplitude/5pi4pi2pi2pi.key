productionVertex :
{
	type = "diffractiveDissVertex";
  beam : {
		name = "pi-";
	};
  target : {
		name = "p+";
	};
};


decayVertex :
{

  XDecay :
	{
		isobars = ({ 
		             name        = "isobarA";
							
			     isobars = ({
				name        = "isobarB";
				fsParticles = ({
						name  = "pi-";
						
					},
					{	
						name  = "pi+";
					});
				},
				{
				name        = "isobarC";
				fsParticles = ({
					    	name  = "pi-";
						},
						{
						name  = "pi+";
					});
					
				});
			});
			fsParticles = ({
				name  = "pi-";
			    	});
	};

};


waveSetParameters :
{
  // X and isobar quantum number ranges
	isospinRange    = [0, 2];
	JRange          = [0, 8];
	MRange          = [0, 0];
	reflectivity    = +1;
	useReflectivity = true;
	allowSpinExotics = true;

	// X and isobar decay quantum number ranges
	LRange = [0, 4];
	SRange = [0, 20];

@include "5piIsobarWhiteList.conf"
 
};
